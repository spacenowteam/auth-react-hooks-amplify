{
    "userPoolName": "spacenowAuthUserPool",
    "autoVerifiedAttributes": [
        "email"
    ],
    "mfaConfiguration": "ON",
    "mfaTypes": [
        "SMS Text Message"
    ],
    "roleName": "spacenowAuthCognito_sns-role",
    "roleExternalId": "spacenowAuthCognito_role_external_id",
    "policyName": "spacenowAuthCognito-sns-policy",
    "smsAuthenticationMessage": "Your authentication code is {####}",
    "smsVerificationMessage": "Your verification code is {####}",
    "emailVerificationSubject": "Your verification code",
    "emailVerificationMessage": "Your verification code is {####}",
    "defaultPasswordPolicy": false,
    "passwordPolicyMinLength": 8,
    "passwordPolicyCharacters": [
        "Requires Lowercase",
        "Requires Uppercase",
        "Requires Numbers",
        "Requires Symbols"
    ],
    "requiredAttributes": [
        "email"
    ],
    "userpoolClientName": "spacenowAuthCognito_app_client",
    "userpoolClientGenerateSecret": true,
    "userpoolClientRefreshTokenValidity": 30,
    "userpoolClientReadAttributes": [
        "email"
    ],
    "mfaLambdaRole": "spacenowAuthCognito_totp_lambda_role",
    "mfaLambdaLogPolicy": "spacenowAuthCognito_totp_lambda_log_policy",
    "mfaPassRolePolicy": "spacenowAuthCognito_totp_pass_role_policy",
    "mfaLambdaIAMPolicy": "spacenowAuthCognito_totp_lambda_iam_policy",
    "userpoolClientLambdaRole": "spacenowAuthCognito_userpoolclient_lambda_role",
    "userpoolClientLogPolicy": "spacenowAuthCognito_userpoolclient_lambda_log_policy",
    "userpoolClientLambdaPolicy": "spacenowAuthCognito_userpoolclient_lambda_iam_policy",
    "userpoolClientSetAttributes": false,
    "useDefault": "manual",
    "authSelections": "userPoolOnly",
    "resourceName": "spacenowAuthCognito",
    "authRoleName": {
        "Ref": "AuthRoleName"
    },
    "unauthRoleName": {
        "Ref": "UnauthRoleName"
    },
    "authRoleArn": {
        "Fn::GetAtt": [
            "AuthRole",
            "Arn"
        ]
    },
    "unauthRoleArn": {
        "Fn::GetAtt": [
            "UnauthRole",
            "Arn"
        ]
    }
}